function loading(){$("#loading").fadeIn(250)}function loaded(){$("#loading").fadeOut(250)}function ajaxCall(e,t,n,o,a,r=0){return ajaxCallFailure(e,t,n,void 0,o,a,r)}function ajaxCallFailure(e,t,n,o,a,r,l=0){return $.ajax({url:t,type:"POST",dataType:"json",data:e,timeout:l,beforeSend:function(e){void 0===a&&loading()},success:function(e){void 0===a&&loaded(),!1===e.error?void 0!==n&&"function"==typeof n&&n(e):void 0!==o&&"function"==typeof o?o(e):alert(e.ret)},complete:function(){void 0!==r&&"function"==typeof r&&r()},error:function(e,t,n){void 0===a&&loaded(),showAjaxAlert(e,t,n)}}),!1}function ajaxLinkCall(e,t,n,o,a){return ret=!0,null!==n&&""!==n&&(ret=confirm(n)),!0===ret&&$.ajax({url:t,type:"POST",dataType:"json",data:e,beforeSend:function(e){loading()},success:function(e){loaded(),!1===e.error?(null!==o&&""!==o&&alert(o),void 0!==a&&"function"==typeof a&&a(e)):alert(e.ret)},error:function(e,t,n){loaded(),showAjaxAlert(e,t,n)}}),!1}function showAjaxAlert(e,t,n){"timeout"===t&&alert(lajax.t("Request timeout. Please try again later.")),0<$.trim(e.responseText).length&&(e="Error:"+e.responseText,null!==t&&(e+=", "+t),e+=", "+n,alert(e))}function ajaxSilentCall(e,t){return $.ajax({url:t,type:"POST",dataType:"json",data:e,success:function(e){},error:function(e,t,n){0<$.trim(e.responseText).length&&(e="Error:"+e.responseText,null!==t&&(e+=", "+t),e+=", "+n,console.log(e))}}),!1}